		"next-compose-plugins": "^2.2.0",
		"next-routes": "^1.4.2",
		"next-runtime-dotenv": "^1.2.0",
		"node-fetch": "^2.6.0",
		"path": "^0.12.7",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"serve": "^11.3.2",
		"siginfo": "^2.0.0",
		"stackback": "0.0.2",
		"stoppable": "^1.1.0",
		"styled-components": "^5.1.1",
		"ts-polyfill": "^3.8.2"
